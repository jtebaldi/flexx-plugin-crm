<div class="row">
  <div class="col-sm-6">
    <p>Form: <strong><%= @form.parent.name %></strong></p>
  </div>
  <div class="col-sm-6">
    <p>Submitted: <strong><%= @form.created_at.in_time_zone(cookies[:timezone]).strftime('%m/%d/%Y - %I:%M %p') %></strong></p>
  </div>
</div>
<h5>Form Contents</h5>
<% @form.parent.fields.each do |f| %>
  <% unless f[:field_type] == 'submit' %>
    <p><%= f[:label] %> - <%= @form.the_settings[:fields][f[:cid]] %></p>
  <% end %>
<% end %>